<?xml version="1.0" encoding="UTF-8" ?>

<!-- ld31 Normal Stage -->

<objects root="AppStage'theStage'">

	<object class="AppStage" name="theStage">
		<stageDimensions>960,0</stageDimensions>

		<children>

			<object class="Scene" name="$^_scene">
				<gravity>0,1000</gravity>
				<children>
					<object class="BodyDefault" name="$^_body1">
						<position>-80,-5</position>
					</object>
					<object class="BodyDefault" name="$^_body2">
						<position>10,1</position>
					</object>
				</children>
				
				<links>[
					{ BodyDefault'$^_body1', BodyDefault'$^_body2' }
				]</links>
			</object>

		</children>			

	</object>
	
</objects>
